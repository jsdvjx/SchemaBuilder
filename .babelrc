{
    "presets": ["es2015", "react","stage-0"],
    "plugins": [
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-decorators",
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": false,
                "regenerator": true,
                "moduleName": "babel-runtime"
            }
        ]
        
    ],
    "comments": false
}
